	<queryString>
		<![CDATA[SELECT  A.ID                         INV_ID         ,
        A.NUMBER    			     INV_NUMBER     , 
		A.DATE  					 TRAN_DATE		,
        A.TYPE      				 PAYMENT_METHOD , 
        B.NAME 						 CASH_NAME 		, 
        ( SELECT NAME 
          FROM   CLIENTS 
          WHERE  ID = A.CLIENT_ID
		) 							 CLIENT_NAME    , 
        C.NAME                       INVENTORY_NAME , 
        U.NAME						 USER_NAME	    ,
        A.DISCOUNT                  				,
        A.TAX                       				, 
        A.TOTAL                    					, 
        A.FINAL_TOTAL               				, 
        A.USER_ID 
FROM 	SALES_INVOICE_HEADER A , 
		CACHES 				 B , 
        INVENTORIES 		 C ,
        USERS 				 U
WHERE 	A.CACHE_ID     =     B.ID
AND	    A.INVENTORY_ID =     C.ID
AND 	A.USER_ID      =     U.ID
and A.ID= $P{id}]]>
	</queryString>
