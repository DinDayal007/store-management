		<parameter name="invoiceId" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT  A.PURCHASE_INVOICE_HEADER_ID,
		A.ITEM_ID , 
		B.NAME ITEM_NAME , 
		B.CODE ITEM_CODE,
		C.NAME  UNIT_NAME , 
		A.PRICE , 
		A.QTY ,
		A.TOTAL 
from 	PURCHASE_INVOICE_DETAILS A , 
		ITEMS B , 
        UNITS C
WHERE 	A.ITEM_ID = B.ID 
AND 	A.UNIT_ID  =  C.ID 
AND PURCHASE_INVOICE_HEADER_ID = $P{invoiceId}]]>
		</queryString>
		<field name="PURCHASE_INVOICE_HEADER_ID" class="java.lang.Integer"/>
